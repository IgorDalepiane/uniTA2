<AnchorPane id="AnchorPane" prefHeight="223.0" prefWidth="267.0" xmlns="http://javafx.com/javafx/8.0.221" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Empresa.Controller.EstoqueDialogController">
    <children>
        <GridPane layoutX="39.0" layoutY="22.0" prefHeight="129.0" prefWidth="189.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="168.0" minWidth="10.0" prefWidth="86.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="248.0" minWidth="10.0" prefWidth="132.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <!-- Pessoa -->
                <Label text="Nome" GridPane.rowIndex="0" />
                <Label text="Descrição" GridPane.rowIndex="1" />
                <Label text="Preço" GridPane.rowIndex="2" />
                <Label text="Quantidade" GridPane.rowIndex="3" />
                <!-- /Pessoa -->


                <!-- Pessoa -->
                <TextField fx:id="textFieldNome" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                <TextField fx:id="textFieldDescricao" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="textFieldPreco" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="textFieldQuantidade" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <!-- /Pessoa -->
            </children>
        </GridPane>
        <Group layoutX="15.0" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0">
            <children>
                <Button fx:id="btnConfirmar" layoutX="129.0" layoutY="137.0" mnemonicParsing="false" onAction="#handleButtonConfirmar" text="Confirmar" />
                <Button fx:id="btnCancelar" layoutX="208.0" layoutY="137.0" mnemonicParsing="false" onAction="#handleButtonCancelar" text="Cancelar" />
            </children>
        </Group>
    </children>
</AnchorPane>
